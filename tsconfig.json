{
  "compilerOptions": {
    "module": "esnext",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "types": [
      "vite",
      "vite-plugin-pwa",
      "vitepress"
    ]
  },
  "exclude": [
    "dist",
    "node_modules",
    "test"
  ]
}
